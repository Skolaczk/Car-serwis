{
    "extends": ["plugin:prettier/recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["warn", {
        "singleQuote": true,
        "endOfLine": "auto",
        "tabWidth": 2
      }]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  }